{
    "name": "ad",   
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["text.js"]
        }
    ],
    "browser_action": {
        "default_popup":"popup.html",
        "default_title":"ML ad detection"
    },
    "web_accessible_resources": [
        "popup.html"
    ],
    "permissions": [
        "http://localhost/*"
    ]
}